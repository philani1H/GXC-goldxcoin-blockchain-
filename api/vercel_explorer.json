{
    "env":  {
                "VERCEL":  "1",
                "DATABASE_PATH":  "/tmp/gxc_explorer.db"
            },
    "builds":  [
                   {
                       "src":  "explorer.py",
                       "use":  "@vercel/python"
                   }
               ],
    "version":  2,
    "rewrites":  [
                     {
                         "destination":  "/explorer.py",
                         "source":  "/(.*)"
                     }
                 ]
}
